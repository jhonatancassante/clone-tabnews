{
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
    "next/core-web-vitals",
    "prettier",
    "plugin:jsonc/recommended-with-jsonc"
  ],
  "plugins": ["no-secrets"],
  "rules": {
    "no-secrets/no-secrets": "error"
  },
  "ignorePatterns": ["package.json", "package-lock.json", "node_modules/"],
  "env": {
    "es2021": true,
    "node": true
  }
}
